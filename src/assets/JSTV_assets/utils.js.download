function checkIE() 
{
    var ua = window.navigator.userAgent;
    var msie = ua.indexOf("MSIE ");
    if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))
    {
    //  var v = parseInt(ua.substring(msie + 5, ua.indexOf(".", msie)));
    //  alert("MSIE = "+v);
        location.href = "assets/info.html";
        return true;
    }
    return false;
}
checkIE();
